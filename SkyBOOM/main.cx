package main

import "gl"
import "glfw"

import "app"
import "draw"
import "entities"
import "inputs"
import "map"
import "mov"
import "space"
import "time"



func main () {
	gameInit()

	for bool.not(glfw.ShouldClose(app.Name)) {
		time.Update()
		entities.Update()
		gameUpdate()
		drawAll()
		
		glfw.PollEvents()
		glfw.SwapBuffers(app.Name)
	}
}
